{
    "name": "qlimix/messagebus-async-middleware",
    "type": "library",
    "description": "Asynchronous handling of message via messagebus middleware",
    "license": "MIT",
    "authors": [
        {
            "name": "frank",
            "email": "frank@qlimix.net"
        }
    ],
    "require": {
        "php": ">=7.2",
        "qlimix/messagebus": "^1.0",
        "qlimix/queue": "^1.0",
        "qlimix/serializable": "^1.0",
        "qlimix/uuid": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Qlimix\\MessageBus\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Qlimix\\Tests\\MessageBus\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "require-dev": {
        "qlimix/code-standard": "^1.0"
    }
}
